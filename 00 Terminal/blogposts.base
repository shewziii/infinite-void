views:
  - type: table
    name: Table
    filters:
      and:
        - file.tags.contains("BLOG")
        - file.folder != "99 Templates"
    order:
      - file.name
      - date-started
      - blog-status
    sort:
      - property: blog-status
        direction: ASC
      - property: date-started
        direction: DESC
    columnSize:
      file.name: 432
      note.date-started: 129
    rowHeight: medium
